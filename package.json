{
  "name": "@recogito/annotorious",
  "version": "2.5.1",
  "description": "A JavaScript image annotation library",
  "main": "dist/annotorious.min.js",
  "scripts": {
    "start": "webpack serve --open --mode=development",
    "build": "webpack --mode=production",
    "build-polyfills": "webpack --mode=production --config webpack.polyfills.js",
    "vite-build": "vite build && mv ./dist/annotorious.umd.js ./dist/annotorious.min.js && mv ./dist/annotorious.umd.js.map ./dist/annotorious.min.js.map && mv ./dist/style.css ./dist/annotorious.min.css",
    "test": "./node_modules/.bin/mocha --require @babel/register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/recogito/annotorious.git"
  },
  "keywords": [
    "Annotation",
    "ImageAnnotation"
  ],
  "author": "Rainer Simon",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/recogito/annotorious/issues"
  },
  "homepage": "https://recogito.github.io/annotorious",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.16",
    "@preact/preset-vite": "^2.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.4",
    "glob-parent": ">=5.1.2",
    "html-webpack-plugin": "^4.5.2",
    "mini-css-extract-plugin": "^1.6.0",
    "mocha": "^7.1.1",
    "node-sass": "^5.0.0",
    "sass": "^1.34.1",
    "sass-loader": "^10.2.0",
    "terser-webpack-plugin": "^4.2.3",
    "vite": "^2.3.7",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@recogito/recogito-client-core": "1.3.2",
    "preact": "^10.5.13",
    "tiny-emitter": "^2.1.0"
  }
}
